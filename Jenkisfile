pipeline {
    agent any
        stages {
             stage ('Build') {
                            steps {
                                   sh "whoiam"
                                   sh "pwd"
                                   sh "which mvn"
                                   sh "which maven"
                                   sh "mvn clean verify"
                                   sh "mvn --version"
                                   sh 'mvn -B clean package'
                                   sh 'mvn gatling:test'
                                  }
                    }
        }
}
