pipeline {
    agent any
        stages {
             stage ('Build') {
                            steps {
                                git url: 'https://github.com/cyrille-leclerc/multi-module-maven-project'
                                withMaven(
                                    maven: 'maven-3', // (1)
                                    mavenLocalRepo: '.repository', // (2)
                                    mavenSettingsConfig: 'my-maven-settings' // (3)
                                ) {
                                  sh "mvn clean verify"
                            }
                }
              }
        }
}
